{
  "name": "pcesports",
  "description": "PC Esports website",
  "version": "4.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxorlovsky/pce-fe"
  },
  "scripts": {
    "clean": "rm -rf ./public/dist",
    "jslint": "node_modules/.bin/eslint --ext .js,.vue \"./src/**/*.{js,vue}\"",
    "stylelint": "node_modules/.bin/stylelint ./styles/**/*.scss, ./src/components/**/*.scss --syntax scss --cache --config stylelint.config.json",
    "webpack": "node_modules/.bin/webpack --progress --config webpack.config.js",
    "build": "yarn clean && yarn jslint && yarn stylelint && yarn webpack",
    "release": "git pull && yarn && yarn jslint && yarn stylelint && node_modules/.bin/webpack --config webpack.config.js --env.production",
    "watch": "node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "axios": "0.19.2",
    "babel-loader": "8.0.5",
    "bootstrap": "4.3.1",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.2",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "image-loader": "0.0.1",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "7.0.0",
    "node-sass-glob-importer": "5.3.1",
    "postcss-loader": "3.0.0",
    "raw-loader": "2.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.20.1",
    "sass-loader": "7.1.0",
    "stylelint": "10.0.1",
    "uglifyjs-webpack-plugin": "2.1.2",
    "vue": "2.6.10",
    "vue-analytics": "5.16.4",
    "vue-directive-tooltip": "1.6.3",
    "vue-loader": "15.7.0",
    "vue-multiselect": "2.1.6",
    "vue-router": "3.0.6",
    "vue-template-compiler": "2.6.10",
    "vuex": "3.1.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.2",
    "webpack-dashboard": "3.0.5",
    "webpack-dev-server": "3.3.1"
  }
}
