{
  "name": "pcesports",
  "description": "PC Esports website",
  "version": "5.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxorlovsky/pcesports.com"
  },
  "dependencies": {
    "eslint": "^8.28.0",
    "git-commit-msg": "^0.3.0",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "tslint"
  ],
  "git-commit-hook": {
    "types": [
