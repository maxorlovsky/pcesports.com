{
  "name": "pcesports",
  "description": "PC Esports website",
  "version": "5.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxorlovsky/pcesports.com"
  },
  "scripts": {
    "tslint": "node_modules/.bin/eslint --ext .ts ./frontend/src/ --ext .ts ./backend/src/"
  },
  "dependencies": {
    "eslint": "^8.28.0",
    "git-commit-msg": "^0.3.0",
    "pre-commit": "^1.2.2",
    "typescript": "^4.9.3"
  },
  "pre-commit": [
    "tslint"
  ],
  "git-commit-hook": {
    "types": [
      "feat",
      "fix",
      "chore",
      "docs",
      "refactor",
      "style",
      "test",
      "revert",
      "merge",
      "balance"
    ],
    "lineLength": 120
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0"
  }
}
